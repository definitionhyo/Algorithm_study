package baekjoon;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int A;
		int B;
		do {
			A = scan.nextInt();
			B = scan.nextInt();
		} while (A > 0 || B > 10); {
				A = scan.nextInt();
				B = scan.nextInt();
			}
			System.out.println(A);
			System.out.println(B);
			System.out.println(A+B);
	}
	
}
