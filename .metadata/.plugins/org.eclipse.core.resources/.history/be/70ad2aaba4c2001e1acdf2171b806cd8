package baekjoon.inoutcalculate;

public class number_10171 {

	public static void main(String[] args) {
		// 이스케이프 문자 사용하는 방법 물어보는 문제인듯
		// / 출력 /, \ 출력 \\
		
		System.out.println("\\    /\\");
		System.out.println(" )  ( ')");
		System.out.println("(  /  )");
		System.out.println(" \\(__)|");
	}

}